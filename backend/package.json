    "inversify": "^7.2.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
